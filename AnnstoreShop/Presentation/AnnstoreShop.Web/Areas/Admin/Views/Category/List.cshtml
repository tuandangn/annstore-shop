@model CategoryListModel
@{
    ViewData["Title"] = "List";
}

<div class="col-12 mt-5">

    <h1>List</h1>

    <table class="table">
        @foreach (var item in Model.Categories)
        {
            <tr>
                <td></td>
                <td>                @item.Name</td>
            </tr>
        }
    </table>
</div>

